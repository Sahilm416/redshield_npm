{
  "name": "redshield",
  "license": "MIT",
  "version": "0.4.9",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./ui": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/esm/components/index.js",
      "require": "./dist/cjs/components/index.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsup && pnpm build:declarations",
    "build:declarations": "npx tsc -p tsconfig.declarations.json",
    "clean": "rmdir /s /q dist",
    "ubuild": "tsup && pnpm build:udeclarations",
    "build:udeclarations": "tsc -p tsconfig.declarations.json",
    "dev": "tsup --watch",
    "lint": "tsc"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.359.0",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.36",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": ">=14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": false,
      "description": "This package requires Next.js version 14.1.0 or higher.",
      "link": "https://nextjs.org/docs/getting-started"
    }
  }
}
