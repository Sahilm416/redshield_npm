{
  "name": "redshield",
  "license": "MIT",
  "version": "0.3.19",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "pnpm clean && tsup && pnpm build:declarations",
    "build:declarations": "npx tsc -p tsconfig.declarations.json",
    "clean": "rmdir /s /q dist",
    "ubuild": "pnpm uclean && pnpm build:udeclarations",
    "build:udeclarations": "tsc -p tsconfig.declarations.json",
    "uclean": "rimraf ./dist",
    "dev": "tsup --watch",
    "lint": "tsc"
  },
  "dependencies": {
    "next": "^14.1.0",
    "tslib": "^2.6.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.18",
    "fs": "0.0.1-security",
    "postcss": "^8.4.36",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}